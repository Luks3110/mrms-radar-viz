# Backend URL for proxying API requests during development
# This is used by Vite's proxy configuration
VITE_BACKEND_URL=http://localhost:8000

# API base URL for frontend
# Use '/api' for development with Vite proxy
# Use full backend URL (e.g., 'http://your-backend-domain:8000/api') for production without proxy
VITE_API_BASE_URL=/api
